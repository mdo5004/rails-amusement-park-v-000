<br><%= form_for attraction do |f|%>
<br><%= f.label :name %>
<br><%= f.text_field :name %>
<br>
<br><%= f.label :min_height, "Minimum Height (in inches)" %>
<br><%= f.text_field :min_height %>
<br><%= f.label :happiness_rating, "How much will it increase happiness? (1-5 scale)" %>
<br><%= f.text_field :happiness_rating %>
<br><%= f.label :nausea_rating, "How nauseating is it? (1-5 scale)" %>
<br><%= f.text_field :nausea_rating %>
<br><%= f.label :tickets, "How many tickets does it requre?" %>
<br><%= f.number_field :tickets %>
<br><br><%= f.submit %>
<% end %>